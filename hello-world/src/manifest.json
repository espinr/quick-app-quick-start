{
  "package": "org.example.demo",
  "name": "hello-world",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1070,
  "icon": "/Common/logo.png",
  "deviceTypeList": [
    "phone"
  ],
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "log"
  },
  "router": {
    "entry": "StartPage",
    "pages": {
      "StartPage": {
        "component": "index"
      },
      "DetailPage": {
        "component": "index"
      }
    },
    "widgets": {
      "WidgetDemo": {
        "name": "Widget Demo",
        "description": "Quick App widget display",
        "component": "index",
        "path": "/WidgetDemo",
        "features": []
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": false,
    "menuBarData": {
      "menuBar": true
    },
    "pages": {
      "Demo": {
        "titleBarText": "Start Page"
      },
      "DemoDetail": {
        "titleBarText": "Details Page"
      }
    }
  }
}